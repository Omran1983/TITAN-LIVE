{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "DoctorFixResult",
    "type": "object",
    "required": [
        "signal_id",
        "action",
        "outcome",
        "duration_ms",
        "notes"
    ],
    "properties": {
        "signal_id": {
            "type": "string"
        },
        "action": {
            "type": "string"
        },
        "outcome": {
            "enum": [
                "SUCCESS",
                "FAILED",
                "ABORTED"
            ]
        },
        "duration_ms": {
            "type": "integer",
            "minimum": 0
        },
        "notes": {
            "type": "string"
        }
    }
}